/* < DTS2014072908570 l00133898 20140804 begin */
&tlmm_pinmux {

	kx023_int1_pin {
		qcom,pins = <&gp 115>;
		qcom,num-grp-pins = <1>;
		label = "kx023_int_pin";
		kx023_int1_default: int1_default {
			  drive-strength = <6>;
			  bias-pull-dowm;
		};
		kx023_int1_sleep: int1_sleep {
			drive-strength = <2>;
			bias-pull-down;
		};
	};
	apds9930_int_pin {
		qcom,pins = <&gp 113>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "apds9930-irq";
		apds9930_default: apds9930_default {
			drive-strength = <6>;
			bias-pull-up;
		};
		apds9930_sleep: apds9930_sleep {
			drive-strength = <2>;
			bias-pull-down;
		};
	};
    /* DTS2014081306069 modified by guoling WX232221 start */
    /* < DTS2014072402059 wanghang 20140806 begin */
    rpr521_int_pin {
        qcom,pins = <&gp 113>;
        qcom,pin-func = <0>;
        qcom,num-grp-pins = <1>;
        label = "rpr521-irq";
        rpr521_default: rpr521_default {
            drive-strength = <6>;
            bias-pull-up;
        };
        rpr521_sleep: rpr521_sleep {
            drive-strength = <2>;
            bias-pull-down;
        };
    };
    /* < DTS2014072402059 wanghang 20140806 end */
    /* DTS2014081306069 modified by guoling WX232221 end */
};

